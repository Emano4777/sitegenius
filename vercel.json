{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook/?",
      "methods": ["POST"],
      "headers": {
        "cache-control": "no-cache"
      },
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "trailingSlash": false
}
